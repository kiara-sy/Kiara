1.
#include <stdio.h>
int main() {
    int A, B;
    scanf("%d %d", &A, &B);
    printf("%d\n", A + B);
    return 0;
}

2.
#include <stdio.h>

int main() {
    char id[51];
    scanf("%s", id); 
    printf("%s??!\n", id); 
    return 0;
}

3.
#include <stdio.h>
#include <string.h>

int main() {
    long long A, B, C;
    scanf("%lld %lld %lld", &A, &B, &C); 

    if (1 <= A, B, C <= 10 ^ 12) {
        printf("%lld\n", A + B + C);
    }
    return 0;
}

4.
#include <stdio.h>
#include <string.h>

int main() {

        printf("\\    /\\\n");
        printf(" )  ( ')\n");
        printf("(  /  )\n");
        printf(" \\(__)|\n");
    return 0;
}

5.
#include <stdio.h>
#include <string.h>

int main() {
        printf("|\\_/|\n");
        printf("|q p|   /}\n");
        printf("( 0 )\"\"\"\\\n");
        printf("|\"^\"`    |\n");
        printf("||_/=\\\\__|\n");
    return 0;
}
