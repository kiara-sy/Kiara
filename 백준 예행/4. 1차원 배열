1.
#include <stdio.h>

int main() {
    int N, v, count = 0;
    int arr[100];
    
    scanf("%d", &N);
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }
    scanf("%d", &v);

    for (int i = 0; i < N; i++) {
        if (arr[i] == v) {
            count++;
        }
    }
    printf("%d\n", count);

    return 0;
}

2.
#include <stdio.h>

int main() {
    int N, X;
    int arr[10000];

    scanf("%d %d", &N, &X);
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }
    for (int i = 0; i < N; i++) {
        if (arr[i] < X) {
            printf("%d ", arr[i]);
        }
    }
    return 0;
}

3.
#include <stdio.h>
#include <limits.h>

int main() {
    int N, num;
    int min = INT_MAX;
    int max = INT_MIN;

    scanf("%d", &N);
    for (int i = 0; i < N; i++) {
        scanf("%d", &num);

        if (num < min) min = num;
        if (num > max) max = num;
    }

    printf("%d %d\n", min, max);

    return 0;
}

4.
#include <stdio.h>

int main() {
    int arr[9];
    int max = 0;
    int index = 0;

    for (int i = 0; i < 9; i++) {
        scanf("%d", &arr[i]);

        if (arr[i] > max) {
            max = arr[i];
            index = i + 1;
        }
    }

    printf("%d\n%d\n", max, index);

    return 0;
}

5.
#include <stdio.h>

int main() {
    int N, M;
    scanf("%d %d", &N, &M);

    int basket[101] = { 0 };

    for (int m = 0; m < M; m++) {
        int i, j, k;
        scanf("%d %d %d", &i, &j, &k);
        for (int b = i; b <= j; b++) {
            basket[b] = k; 
        }
    }

    for (int n = 1; n <= N; n++) {
        printf("%d ", basket[n]);
    }

    return 0;
}

6.
#include <stdio.h>

int main() {
    int N, M;
    scanf("%d %d", &N, &M);
    int basket[101];
    for (int i = 1; i <= N; i++) {
        basket[i] = i;
    }
    for (int k = 0; k < M; k++) {
        int i, j;
        scanf("%d %d", &i, &j);

        int temp = basket[i];
        basket[i] = basket[j];
        basket[j] = temp;
    }
    for (int i = 1; i <= N; i++) {
        printf("%d ", basket[i]);
    }

    return 0;
}

7.
#include <stdio.h>

int main() {
    int students[31] = { 0 };
    int num;
    for (int i = 0; i < 28; i++) {
        scanf("%d", &num);
        students[num] = 1;
    }

    for (int i = 1; i <= 30; i++) {
        if (students[i] == 0) {
            printf("%d\n", i);
        }
    }
    return 0;
}

8.
#include <stdio.h>

int main() {
    int arr[10];
    int remainder[42] = { 0 };
    int input, count = 0;

    for (int i = 0; i < 10; i++) {
        scanf("%d", &input);
        int r = input % 42;
        remainder[r] = 1;
    }
    for (int i = 0; i < 42; i++) {
        if (remainder[i] == 1) {
            count++; 
        }
    }
    printf("%d\n", count);

    return 0;
}

9.
#include <stdio.h>

int main() {
    int N, M;
    scanf("%d %d", &N, &M);
    int basket[101]; 

    for (int i = 1; i <= N; i++) {
        basket[i] = i;
    }
    for (int m = 0; m < M; m++) {
        int i, j;
        scanf("%d %d", &i, &j);
        while (i < j) {
            int temp = basket[i];
            basket[i] = basket[j];
            basket[j] = temp;
            i++;
            j--;
        }
    }

    for (int i = 1; i <= N; i++) {
        printf("%d ", basket[i]);
    }
    return 0;
}

10.
#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);
    int scores[1000];
    int max_score = 0;

    for (int i = 0; i < N; i++) {
        scanf("%d", &scores[i]);
        if (scores[i] > max_score) {
            max_score = scores[i];
        }
    }
    double sum = 0.0;

    for (int i = 0; i < N; i++) {
        sum += (double)scores[i] / max_score * 100.0;
    }
    printf("%.2lf\n", sum / N);

    return 0;
}
